Config {
  font = "xft:DejaVu Sans Mono:size=6:antialias=true",
  fgColor = "#fff",
  template = " %StdinReader% }{ %battery% %date% ",
  commands = [
    Run Battery [
      "--template", "<left>%",
      "--Low", "15",
      "--low", "red"] 50,
    Run Date "%-m-%d %k:%M" "date" 10,
    Run StdinReader
  ]
}