{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "workbench.colorTheme": "Default Light+",
  "editor.rulers": [80],
  "workbench.colorCustomizations": {
    "statusBar.background": "#999999"
  },
  "editor.minimap.enabled": false,
  "typescript.referencesCodeLens.enabled": true,
  "cSpell.userWords": [
    "Brotli",
    "buttonish",
    "Cacheable",
    "cels",
    "esbuild",
    "lingui",
    "middlewares",
    "niedzielski",
    "nums",
    "oidoid",
    "proto",
    "protobuf",
    "protos",
    "stateful",
    "testid",
    "unmarshal",
    "vitest"
  ],
  // "explorer.confirmDelete": false,
  // "explorer.confirmDragAndDrop": false,
  "typescript.reportStyleChecksAsWarnings": false,
  "search.smartCase": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "files.exclude": {
    ".eslintcache": true,
    "package-lock.json": true
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "prettier.requireConfig": true,
  "editor.formatOnSave": true,
  "debug.openDebug": "neverOpen",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.referencesCodeLens.enabled": true,
  "search.useGlobalIgnoreFiles": true,
  "typescript.implementationsCodeLens.enabled": true,
  "[plaintext]": {
    "editor.wordBasedSuggestions": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.wordWrap": "on"
  },
  "window.titleBarStyle": "custom",
  "editor.renderFinalNewline": true,
  "references.preferredLocation": "view",
  "files.autoSave": "off",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.addMissingImports": true,
    "source.organizeImports": true
  },
  "cSpell.logLevel": "Information",
  "workbench.editor.tabSizing": "shrink",
  "editor.unfoldOnClickAfterEndOfLine": true,
  // "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "javascriptvue",
    "typescript",
    "typescriptreact",
    "typescriptvue"
  ],
  "eslint.codeAction.disableRuleComment": {},
  "editor.renderWhitespace": "trailing",
  "workbench.editor.pinnedTabSizing": "shrink",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "window.restoreWindows": "preserve",
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "editor.codeLensFontSize": 5,
  "editor.stickyTabStops": true,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "editor.lineNumbers": "off",
  "editor.selectionClipboard": false,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.bracketPairColorization.enabled": true,
  "workbench.tree.indent": 12,
  "editor.fontFamily": "DejaVu Sans Mono",
  "files.associations": {
    // always allow comments
    "*.json": "jsonc",
    "*.glsl.ts": "glsl",
    "*.make": "makefile"
  },
  //"explorer.excludeGitIgnore": true,
  "cSpell.customDictionaries": {
    "custom-dictionary-user": {
      "name": "custom-dictionary-user",
      "path": "~/.cspell/custom-dictionary-user.txt",
      "addWords": true,
      "scope": "user"
    }
  },
  "window.commandCenter": true,
  "go.survey.prompt": false,

  "deno.codeLens.testArgs": [
    "--allow-read"
  ],
  "deno.suggest.imports.hosts": {
    "https://deno.land": true
  },

  "gopls": {
    "ui.navigation.importShortcut": "Definition"
  },
  "window.zoomLevel": 2,
  "go.coverOnTestPackage": false,
  "go.enableCodeLens": {
    "runtest": false
  },
  "workbench.activityBar.visible": false,
  "deno.inlayHints.parameterNames.enabled": "none",
  "deno.inlayHints.variableTypes.enabled": false,
  "deno.inlayHints.propertyDeclarationTypes.enabled": false,
  "deno.inlayHints.parameterTypes.enabled": false,
  "deno.inlayHints.functionLikeReturnTypes.enabled": false,
  "deno.inlayHints.enumMemberValues.enabled": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
}
