{
  // "typescript.disableAutomaticTypeAcquisition": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.fontSize": 12,
  "window.zoomLevel": 2,
  "workbench.colorTheme": "Default Light Modern",
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "workbench.activityBar.location": "top",
  "editor.rulers": [
    80
  ],
  "references.preferredLocation": "view",
    // no snippets in completion menu like `#region`.
  "editor.snippetSuggestions": "none",
  // "editor.suggest.showKeywords": false, this breaks completion. causes `this` to complete `thiRANDOMTHINGINWITHTHATPREFIX` even though I don't tab.
  "window.titleBarStyle": "custom",
  "workbench.editor.tabSizing": "shrink",
  // no middle click paste.
  "editor.selectionClipboard": false,
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.requireConfig": true,
  // Don't show abc suggestions in autocomplete. These are extremely noisy,
  // often misleading, 99.9% of the time useless.
  "editor.suggest.showWords": false,
  "editor.wordBasedSuggestions": "off",
  "javascript.suggest.names": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.biome": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit",
    "source.fixAll.ts": "explicit",
    // "source.removeUnused.ts": "explicit", this can delete code
    "source.addMissingImports.ts": "always",
    "source.addMissingImports": "always",
    "source.removeUnusedImports": "explicit",
    // "source.organizeImports": "explicit",
    "quickfix.biome": "explicit",
    // "source.organizeImports.biome": "explicit"
  },
  "typescript.tsdk": "node_modules/typescript/lib",

  // Never peek.
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "testing.automaticallyOpenPeekView": "never",
  "gopls": {
    "ui.navigation.importShortcut": "Definition"
  },
  // Don't show typos in the problem pane.
  "cSpell.diagnosticLevel": "Hint",
  "files.associations": {
    "*.glsl.ts": "glsl",
    "*.make": "makefile",
    // always allow comments
    "*.json": "jsonc"
  },
  "workbench.startupEditor": "none",
  "svelte.enable-ts-plugin": true,
  // Always restore prior state.
  "window.restoreWindows": "preserve",
  "go.toolsManagement.autoUpdate": true,
  "cSpell.userWords": [
    "Devvit",
    "Hitbox",
    "hitboxes",
    "Millis",
    "Niedzielski",
    "nonsquare",
    "oidoid",
    "spritesheet",
    "tileset",
    "tilesheet",
    "Voronoi"
  ],
  // "typescript.reportStyleChecksAsWarnings": false,
  // "search.smartCase": true,
  // "js/ts.implicitProjectConfig.checkJs": true,
  // "debug.openDebug": "neverOpen",
  // "search.useGlobalIgnoreFiles": true,
  // "typescript.implementationsCodeLens.enabled": true,
  "[markdown]": {
    "cSpell.diagnosticLevel": "Error"
  },
  "[plaintext]": {
    "cSpell.diagnosticLevel": "Error",
    "editor.wordBasedSuggestions": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.wordWrap": "on"
  },
  "lit-plugin.strict": true,
  "lit-plugin.rules.no-missing-element-type-definition": "error",
  "lit-plugin.rules.no-unknown-event": "error",
  "lit-plugin.rules.no-boolean-in-attribute-binding": "error",
  "lit-plugin.rules.no-complex-attribute-binding": "error",
  "lit-plugin.rules.no-incompatible-property-type": "error",
  "lit-plugin.rules.no-invalid-attribute-name": "error",
  "lit-plugin.rules.no-incompatible-type-binding": "error",
  "lit-plugin.rules.no-invalid-boolean-binding": "error",
  "lit-plugin.rules.no-invalid-directive-binding": "error",
  "lit-plugin.rules.no-unclosed-tag": "error",
  "lit-plugin.rules.no-unknown-attribute": "error",
  "lit-plugin.rules.no-unknown-property": "error",
  "lit-plugin.rules.no-unknown-slot": "error",
  "lit-plugin.rules.no-unknown-tag-name": "error",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "terminal.integrated.enableFileLinks": "on"
  // "lit-plugin.logging": "verbose",
  // "editor.unfoldOnClickAfterEndOfLine": true,
